{
  "manifest_version": 3,
  "name": "PinItUp",
  "version": "1",
  "description": "Modern bookmarking extension",
  "icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "action": {
      "default_icon":{
          "16": "icon16.png",
          "24": "icon24.png", 
          "32": "icon32.png",
          "64": "icon64.png",
          "128": "icon128.png"  

      },
      "default_title":"PinItUp", 
      "default_popup":"index.html"
  },
  "permissions":[
    "tabs", 
    "storage",
    "unlimitedStorage"
  ]
}